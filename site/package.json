        "laravel-mix": "^1.0",
        "lodash": "^4.17.4",
        "popper.js": "^1.12.6",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "particles.js": "^2.0.0",
        "vue-parallaxy": "^1.0.4",
        "vue-particles": "^1.0.9",
        "vue-typer": "^1.0.6",
        "x-particles": "^1.0.8"
